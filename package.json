{
  "name": "mailer.felipemiramontesr.net",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vue-tsc -b && vite build",
    "dev": "vite",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "lint-staged": {
    "*.{js,ts,vue,json,css,md}": [
      "prettier --write"
    ],
    "src/**/*.{ts,vue}": [
      "vitest related --run"
    ]
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "axios": "^1.13.4",
    "emailjs-com": "^3.2.0",
    "lucide-vue-next": "^0.563.0",
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "husky": "^9.1.7",
    "jsdom": "^28.0.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^3.0.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.1.4"
  }
}
